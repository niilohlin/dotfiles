{
    "languageserver": {
        "haskell": {
            "command": "haskell-language-server-wrapper",
            "args": [
                "--lsp"
            ],
            "rootPatterns": [
                ".stack.yaml",
                ".hie-bios",
                "BUILD.bazel",
                "cabal.config",
                "package.yaml"
            ],
            "filetypes": [
                "hs",
                "lhs",
                "haskell"
            ]
        },
        "sourcekit-lsp": {
            "filetypes": ["swift"],
            "command": "sourcekit-lsp"
        },
        "lua": {
            "command": "/Users/niil/workspace/lua-language-server/bin/lua-language-server",
            "args": [
                "-E",
                "/Users/niil/workspace/lua-language-server/bin/main.lua"
            ],
            "filetypes": ["lua"],
            "settings": {
                "Lua": {
                    "workspace.library": {
                    },
                    "diagnostics": {
                        "globals": [ "vim" ]
                    }
                }
            }
        }
    }
}
